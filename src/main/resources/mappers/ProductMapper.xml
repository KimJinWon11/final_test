<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="ProductMapper">
  
  <insert id="insert" parameterType="com.intranet.product.ProductDTO">
  
  insert into product values(#{outsourcing},#{item},#{price},#{count},#{in_date})
  
  </insert>
  
  <select id="list" resultType="com.intranet.product.ProductDTO">
  
  select * from product
  
  </select>
  

  
  </mapper>